{
  "name": "lyfter",
  "version": "1.0.0",
  "description": "Lyfter is a full-stack application with a React frontend and an Express backend.",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "client": "cd client && npm start",
    "server": "cd server && nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.22"
  }
}
